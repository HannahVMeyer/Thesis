title = {{Vertebrate Protein CTCF and its Multiple Roles in a Large-Scale Regulation of Genome Activity.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2729993{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {10},
year = {2009}
}
