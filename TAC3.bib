keywords = {Alleles,Chromosomes, Human, X,Chromosomes, Human, X: genetics,Female,Humans,Linkage Disequilibrium,Protein Binding,Quantitative Trait Loci,RNA, Untranslated,RNA, Untranslated: genetics,RNA, Untranslated: metabolism,Repressor Proteins,Repressor Proteins: genetics,Repressor Proteins: metabolism},
