pages = {923--34},
pmid = {15923337},
title = {{Local coexpression domains of two to four genes in the genome of Arabidopsis.}},
url = {http://www.plantphysiol.org/cgi/content/abstract/138/2/923},
volume = {138},
year = {2005}
