issn = {1549-5469},
journal = {Genome research},
keywords = {Animals,Blood Pressure,Blood Pressure: genetics,Disease Models, Animal,Female,Gene Targeting,Genes, Modifier,Genetic Predisposition to Disease,Genetic Variation,Genome-Wide Association Study,Haplotypes,Humans,Hypertension,Hypertension: etiology,Hypertension: genetics,Kidney,Kidney: metabolism,Linkage Disequilibrium,Male,Mutation,Phenotype,Polymorphism, Single Nucleotide,Quantitative Trait Loci,Rats,Rats, Sprague-Dawley,Retrospective Studies},
