keywords = {Adult,Aged,Aged, 80 and over,Cluster Analysis,CpG Islands,CpG Islands: genetics,DNA Methylation,DNA Methylation: physiology,Female,Gene Expression Profiling,Humans,Male,Microarray Analysis,Middle Aged,Nerve Tissue,Nerve Tissue: metabolism,Nerve Tissue: physiology,Neurons,Neurons: metabolism,Neurons: physiology,Organ Specificity,Organ Specificity: genetics},
month = {aug},
number = {6},
pages = {527--38},
pmid = {20505344},
title = {{Tissue specific DNA methylation of CpG islands in normal human adult somatic tissues distinguishes neural from non-neural tissues.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3322498{\&}tool=pmcentrez{\&}rendertype=abstract},
