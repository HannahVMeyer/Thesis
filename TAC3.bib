issn = {1471-2350},
journal = {BMC medical genetics},
keywords = {Adult,Cardiovascular Diseases,Cardiovascular Diseases: genetics,Cardiovascular Diseases: physiopathology,Cohort Studies,Disease Susceptibility,Electrocardiography,Female,Genetic Markers,Genetic Variation,Genome, Human,Genotype,Heart Rate,Heart Rate: genetics,Humans,Linear Models,Male,Middle Aged,Phenotype,Polymorphism, Single Nucleotide},
pages = {S7},
pmid = {17903306},
title = {{Genome-wide association study of electrocardiographic and heart rate variability traits: the Framingham Heart Study.}},
