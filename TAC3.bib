file = {:Users/hannah/Documents/Mendeley Desktop/2015{\_}The American Journal of Human Genetics{\_}Vilhj{\'{a}}lmsson et al.pdf:pdf},
issn = {00029297},
journal = {The American Journal of Human Genetics},
month = {oct},
