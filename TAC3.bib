institution = {Department of Immunology, Institute for Cell Biology, University of T{\"{u}}bingen, Auf der Morgenstelle 15, D-72076, T{\"{u}}bingen, Germany.},
journal = {Journal of Immunological Methods},
keywords = {computer algorithm,epitope prediction,hla,peptide,t cell},
number = {1-2},
