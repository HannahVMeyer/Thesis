issn = {1549-5469},
journal = {Genome research},
month = {may},
pages = {gr.192278.115--},
pmid = {25953952},
title = {{The landscape of genomic imprinting across diverse adult human tissues.}},
url = {http://genome.cshlp.org/content/early/2015/05/07/gr.192278.115.abstract},
year = {2015}
}
