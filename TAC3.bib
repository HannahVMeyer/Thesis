@article{Castel2015a,
abstract = {Allelic expression analysis has become important for integrating genome and transcriptome data to characterize various biological phenomena such as {\textless}Emphasis Type="Italic"{\textgreater}cis{\textless}/Emphasis{\textgreater}-regulatory variation and nonsense-mediated decay. We analyze the properties of allelic expression read count data and technical sources of error, such as low-quality or double-counted RNA-seq reads, genotyping errors, allelic mapping bias, and technical covariates due to sample preparation and sequencing, and variation in total read depth. We provide guidelines for correcting such errors, show that our quality control measures improve the detection of relevant allelic expression, and introduce tools for the high-throughput production of allelic expression data from RNA-sequencing data.},
