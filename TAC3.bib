keywords = {Bipolar Disorder,Bipolar Disorder: genetics,Depressive Disorder, Major,Depressive Disorder, Major: genetics,Genetic Testing,Genetic Testing: methods,Genetics, Medical,Genetics, Medical: methods,Humans,Linear Models,Mental Disorders,Mental Disorders: genetics,Multifactorial Inheritance,Multifactorial Inheritance: genetics,Multivariate Analysis,Polymorphism, Single Nucleotide,Polymorphism, Single Nucleotide: genetics,Risk Assessment,Risk Assessment: methods,Schizophrenia,Schizophrenia: genetics},
