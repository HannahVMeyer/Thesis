@article{Wang2009b,
abstract = {Hypertension places a major burden on individual and public health, but the genetic basis of this complex disorder is poorly understood. We conducted a genome-wide association study of systolic and diastolic blood pressure (SBP and DBP) in Amish subjects and found strong association signals with common variants in a serine/threonine kinase gene, STK39. We confirmed this association in an independent Amish and 4 non-Amish Caucasian samples including the Diabetes Genetics Initiative, Framingham Heart Study, GenNet, and Hutterites (meta-analysis combining all studies: n = 7,125, P {\textless} 10(-6)). The higher BP-associated alleles have frequencies {\textgreater} 0.09 and were associated with increases of 3.3/1.3 mm Hg in SBP/DBP, respectively, in the Amish subjects and with smaller but consistent effects across the non-Amish studies. Cell-based functional studies showed that STK39 interacts with WNK kinases and cation-chloride cotransporters, mutations in which cause monogenic forms of BP dysregulation. We demonstrate that in vivo, STK39 is expressed in the distal nephron, where it may interact with these proteins. Although none of the associated SNPs alter protein structure, we identified and experimentally confirmed a highly conserved intronic element with allele-specific in vitro transcription activity as a functional candidate for this association. Thus, variants in STK39 may influence BP by increasing STK39 expression and consequently altering renal Na(+) excretion, thus unifying rare and common BP-regulating alleles in the same physiological pathway.},
author = {Wang, Ying and O'Connell, Jeffrey R and McArdle, Patrick F and Wade, James B and Dorff, Sarah E and Shah, Sanjiv J and Shi, Xiaolian and Pan, Lin and Rampersaud, Evadnie and Shen, Haiqing and Kim, James D and Subramanya, Arohan R and Steinle, Nanette I and Parsa, Afshin and Ober, Carole C and Welling, Paul A and Chakravarti, Aravinda and Weder, Alan B and Cooper, Richard S and Mitchell, Braxton D and Shuldiner, Alan R and Chang, Yen-Pei C},
doi = {10.1073/pnas.0808358106},
file = {:Users/hannah/Documents/Mendeley Desktop/2009{\_}Proceedings of the National Academy of Sciences of the United States of America{\_}Wang et al.pdf:pdf},
isbn = {10.1073/pnas.0808358106},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Blood Pressure,Blood Pressure: genetics,Diastole,European Continental Ancestry Group,European Continental Ancestry Group: ethnology,European Continental Ancestry Group: genetics,Gene Frequency,Genetic Predisposition to Disease,Genetic Predisposition to Disease: ethnology,Genetic Predisposition to Disease: genetics,Genome-Wide Association Study,Humans,Hypertension,Hypertension: ethnology,Hypertension: genetics,Nephrons,Nephrons: chemistry,Polymorphism, Single Nucleotide,Protein Binding,Protein-Serine-Threonine Kinases,Protein-Serine-Threonine Kinases: analysis,Protein-Serine-Threonine Kinases: genetics,Sodium,Sodium Chloride Symporters,Sodium: urine,Systole},
language = {en},
month = {jan},
