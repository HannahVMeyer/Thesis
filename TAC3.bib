isbn = {info:doi/10.1371/journal.pcbi.1000770},
issn = {1553-7358},
journal = {PLoS computational biology},
keywords = {Animals,Bayes Theorem,Databases,Gene Expression,Genetic,Humans,Internet,Markov Chains,Mice,Models,Phenotype,Quantitative Trait Loci,Reproducibility of Results,Software,Statistical,Yeasts},
language = {en},
month = {may},
