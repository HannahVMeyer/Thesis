issn = {1540-8140},
journal = {The Journal of cell biology},
keywords = {Actins,Actins: metabolism,Actomyosin,Actomyosin: metabolism,Animals,Cell Adhesion,Cytoskeleton,Cytoskeleton: metabolism,Focal Adhesions,Focal Adhesions: metabolism,HeLa Cells,Humans,Integrins,Integrins: metabolism,Mice,Paxillin,Paxillin: metabolism,Protein Binding,Protein Structure, Tertiary,Protein Transport,Signal Transduction,Talin,Talin: metabolism,Time Factors,Vinculin,Vinculin: metabolism},
month = {dec},
