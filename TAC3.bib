@article{Aljabar2012a,
abstract = {The term manifold learning encompasses a class of machine learning techniques that convert data from a$\backslash$nhigh to lower dimensional representation while respecting the intrinsic geometry of the data. The intuition$\backslash$nunderlying the use of manifold learning in the context of image analysis is that, while each image may be$\backslash$nviewed as a single point in a very high-dimensional space, a set of such points for a population of images$\backslash$nmay be well represented by a sub-manifold of the space that is likely to be non-linear and of a$\backslash$nsignificantly lower dimension. Recently, manifold learning techniques have begun to be applied to the$\backslash$nfield of medical image analysis. In this chapter we will review the most popular manifold learning$\backslash$ntechniques such as Multi-dimensional scaling (MDS), Isomap, Local linear embedding and Laplacian$\backslash$neigenmaps. We will also demonstrate how these techniques can be used for image registration,$\backslash$nsegmentation and biomarker discovery from medical images.},
author = {Aljabar and Wolz and Rueckert},
