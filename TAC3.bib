journal = {Pediatric research},
keywords = {Anatomic,Animals,Aorta,Biological,Congenital,Congenital: embryology,Heart,Heart Defects,Heart Ventricles,Heart Ventricles: embryology,Heart: embryology,Humans,Mesoderm,Models,Pericardium,Pericardium: metabolism,Phenotype,Thoracic,Thoracic: embryology},
