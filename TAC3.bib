journal = {Journal of autoimmunity},
keywords = {Aire,Autoimmune regulator,Gene co-expression,Medullary thymic epithelial cells,PWMs,Promiscuous gene expression,TRA,Tissue-restricted antigen,Tolerance,cTEC,cortical thymic epithelial cell,mTEC,medullary thymic epithelial cell,pGE,position weight matrices,promiscuous gene expression,tissue-restricted antigen},
month = {oct},
pmid = {26481130},
title = {{Evolutionary conserved gene co-expression drives generation of self-antigen diversity in medullary thymic epithelial cells.}},
