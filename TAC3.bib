keywords = {Adipose Tissue,Adipose Tissue: metabolism,Cell Line,Cells, Cultured,Data Interpretation, Statistical,Female,Gene Expression Profiling,Genes, Regulator,Genes, Regulator: genetics,Genotype,Humans,Organ Specificity,Organ Specificity: genetics,Phenotype,Quantitative Trait Loci,Quantitative Trait Loci: genetics,Skin,Skin: metabolism,Twins},
