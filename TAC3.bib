journal = {Human genetics},
keywords = {Algorithms,Databases as Topic,Genetic Markers,Genome-Wide Association Study,Genotype,HapMap Project,Humans,Oligonucleotide Array Sequence Analysis,Polymorphism, Single Nucleotide,Single Nucleotide,Software,Statistics as Topic},
month = {may},
number = {5},
pages = {747--56},
pmid = {22143225},
title = {{Evaluating the effective numbers of independent tests and significant p-value thresholds in commercial genotyping arrays and public imputation reference datasets.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3325408{\&}tool=pmcentrez{\&}rendertype=abstract},
