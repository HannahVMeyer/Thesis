journal = {American journal of human genetics},
keywords = {Algorithms,Computer Simulation,Genome, Human,Genome-Wide Association Study,Genotype,Humans,Linkage Disequilibrium,Models, Genetic,Multifactorial Inheritance,Multifactorial Inheritance: genetics,Pedigree,Polymorphism, Single Nucleotide},
language = {English},
month = {dec},
