issn = {1476-4687},
journal = {Nature},
keywords = {Animals,Biosensing Techniques,Calibration,Cattle,Cell Line,Cell Movement,Cell Movement: physiology,Fluorescent Dyes,Focal Adhesions,Focal Adhesions: metabolism,Humans,Mice,Microscopy, Confocal,Movement,Optical Tweezers,Spectrometry, Fluorescence,Stress, Mechanical,Vinculin,Vinculin: chemistry,Vinculin: deficiency,Vinculin: genetics,Vinculin: metabolism},
month = {jul},
number = {7303},
pages = {263--6},
pmid = {20613844},
title = {{Measuring mechanical tension across vinculin reveals regulation of focal adhesion dynamics.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2901888{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {466},
year = {2010}
}
