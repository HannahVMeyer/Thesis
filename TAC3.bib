journal = {PLoS genetics},
keywords = {Autoimmune Diseases,Autoimmune Diseases: genetics,Autoimmune Diseases: pathology,Bayes Theorem,Diabetes Mellitus, Type 1,Diabetes Mellitus, Type 1: genetics,Diabetes Mellitus, Type 1: pathology,Genetic Predisposition to Disease,Genome, Human,Genome-Wide Association Study,Genotype,Humans,Liver Cirrhosis, Biliary,Liver Cirrhosis, Biliary: genetics,Liver Cirrhosis, Biliary: pathology,Phenotype,Polymorphism, Single Nucleotide},
month = {feb},
