@article{Deelen2014,
abstract = {The genetic contribution to the variation in human lifespan is ∼25{\%}. Despite the large number of identified disease-susceptibility loci, it is not known which loci influence population mortality. We performed a genome-wide association meta-analysis of 7729 long-lived individuals of European descent (≥85 years) and 16 121 younger controls ({\textless}65 years) followed by replication in an additional set of 13 060 long-lived individuals and 61 156 controls. In addition, we performed a subset analysis in cases aged ≥90 years. We observed genome-wide significant association with longevity, as reflected by survival to ages beyond 90 years, at a novel locus, rs2149954, on chromosome 5q33.3 (OR = 1.10, P = 1.74 × 10(-8)). We also confirmed association of rs4420638 on chromosome 19q13.32 (OR = 0.72, P = 3.40 × 10(-36)), representing the TOMM40/APOE/APOC1 locus. In a prospective meta-analysis (n = 34 103), the minor allele of rs2149954 (T) on chromosome 5q33.3 associates with increased survival (HR = 0.95, P = 0.003). This allele has previously been reported to associate with low blood pressure in middle age. Interestingly, the minor allele (T) associates with decreased cardiovascular mortality risk, independent of blood pressure. We report on the first GWAS-identified longevity locus on chromosome 5q33.3 influencing survival in the general European population. The minor allele of this locus associates with low blood pressure in middle age, although the contribution of this allele to survival may be less dependent on blood pressure. Hence, the pleiotropic mechanisms by which this intragenic variation contributes to lifespan regulation have to be elucidated.},
author = {Deelen, Joris and Beekman, Marian and Uh, Hae Won and Broer, Linda and Ayers, Kristin L. and Tan, Qihua and Kamatani, Yoichiro and Bennet, Anna M. and Tamm, Riin and Trompet, Stella and Guobjartsson, Dan{\'{i}}el F. and Flachsbart, Friederike and Rose, Giuseppina and Viktorin, Alexander and Fischer, Krista and Nygaard, Marianne and Cordell, Heather J. and Crocco, Paolina and {Van Den Akker}, Erik B. and B{\"{o}}hringer, Stefan and Helmer, Quinta and Nelson, Christopher P. and Saunders, Gary I. and Alver, Maris and Andersen-Ranberg, Karen and Breen, Marie E. and {van Der Breggen}, Ruud and Caliebe, Amke and Capri, Miriam and Cevenini, Elisa and Collerton, Joanna C. and Dato, Serena and Davies, Karen and Ford, Ian and Gampe, Jutta and Garagnani, Paolo and de Geus, Eco J C and Harrow, Jennifer and {Van Heemst}, Diana and Heijmans, Bastiaan T. and Heinsen, Femke Anouska and Hottenga, Jouke Jan and Hofman, Albert and Jeune, Bernard and Jonsson, Palmi V. and Lathrop, Mark and Lechner, Doris and Martin-Ruiz, Carmen and Mcnerlan, Susan E. and Mihailov, Evelin and Montesanto, Alberto and Mooijaart, Simon P. and Murphy, Anne and Nohr, Ellen a. and Paternoster, Lavinia and Postmus, Iris and Rivadeneira, Fernando and Ross, Owen a. and Salvioli, Stefano and Sattar, Naveed and Schreiber, Stefan and Stef{\'{a}}nsson, Hreinn and Stott, David J. and Tiemeier, Henning and Uitterlinden, Andr{\'{e}} G. and Westendorp, Rudi G J and Willemsen, Gonneke and Samani, Nilesh J. and Galan, Pilar and S{\o}rensen, Thorkild I a and Boomsma, Dorret I. and {Wouter Jukema}, J. and Rea, Irene Maeve and Passarino, Giuseppe and de Craen, Anton J M and Christensen, Kaare and Nebel, Almut and Stef{\'{a}}nsson, K{\'{a}}ri and Metspalu, Andres and Magnusson, Patrik and Blanch{\'{e}}, H{\'{e}}l{\`{e}}ne and Christiansen, Lene and Kirkwood, Thomas B L and {Van Duijn}, Cornelia M. and Franceschi, Claudio and Houwing-Duistermaat, Jeanine J. and Slagboom, P. Eline},
