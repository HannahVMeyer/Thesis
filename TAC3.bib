issn = {1876-7591},
journal = {JACC. Cardiovascular imaging},
keywords = {Chagas Cardiomyopathy,Chagas Cardiomyopathy: pathology,Cine,Heart Ventricles,Heart Ventricles: pathology,Heart Ventricles: ultrasonography,Humans,Hypertrophy,Imaging,Left Ventricular,Left Ventricular: diagnosis,Magnetic Resonance Imaging,Risk Assessment,Three-Dimensional},
month = {aug},
number = {8},
pages = {837--48},
pmid = {22897998},
publisher = {Journal of the American College of Cardiology},
title = {{LV mass assessed by echocardiography and CMR, cardiovascular outcomes, and medical practice.}},
