pmid = {16077740},
shorttitle = {Heredity},
title = {{Adjusting multiple testing in multilocus analyses using the eigenvalues of a correlation matrix.}},
url = {http://dx.doi.org/10.1038/sj.hdy.6800717},
volume = {95},
year = {2005}
}
