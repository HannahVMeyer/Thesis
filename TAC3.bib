keywords = {Adolescent,Adult,Aged,Aged, 80 and over,Anisotropy,Brain,Brain Mapping,Diffusion Tensor Imaging,Female,Humans,Image Processing, Computer-Assisted,Male,Middle Aged,Pilot Projects,Registries,Young Adult},
month = {nov},
pages = {455--69},
pmid = {23629049},
title = {{Multi-site genetic analysis of diffusion images and voxelwise heritability analysis: a pilot project of the ENIGMA-DTI working group.}},
