volume = {18},
year = {2000}
}
@article{VanStipdonk2001,
abstract = {In defense of the host, the immune system must often raise an effective cytotoxic T lymphocyte (CTL) response from a small number of clonal precursors. The degree to which activation stimuli regulate the expansion and differentiation of na{\"{i}}ve CTLs, however, remains unknown. Using an engineered antigen-presenting cell (APC) system that allows control over antigenic stimulation, we studied the signaling duration requirements for priming and clonal expansion of na{\"{i}}ve CTLs. We found that na{\"{i}}ve CTLs become committed after as little as 2 h of exposure to APCs and that their subsequent division and differentiation can occur without the need for further antigenic stimulation of the daughter cells, whether priming is in vitro or in vivo. These data show that after a brief interaction with stimulatory APCs, na{\"{i}}ve CTLs initiate a program for their autonomous clonal expansion and development into functional effectors.},
author = {van Stipdonk, M J and Lemmens, E E and Schoenberger, S P},
doi = {10.1038/87730},
issn = {1529-2908},
journal = {Nature immunology},
keywords = {Animals,Antigen Presentation,Antigen Presentation: immunology,Antigen-Presenting Cells,Antigens, CD28,Antigens, CD28: immunology,Antigens, CD80,Antigens, CD80: immunology,Cell Differentiation,Cell Division,Homeodomain Proteins,Homeodomain Proteins: genetics,Lymphocyte Activation,Mice,Mice, Inbred C57BL,Mice, Mutant Strains,Ovalbumin,Ovalbumin: immunology,Receptors, Antigen, T-Cell,T-Lymphocytes, Cytotoxic,T-Lymphocytes, Cytotoxic: cytology,T-Lymphocytes, Cytotoxic: immunology},
month = {may},
