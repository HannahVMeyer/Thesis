keywords = {Adult,Bayes Theorem,Genome-Wide Association Study,Genome-Wide Association Study: methods,Humans,Lipoproteins,Lipoproteins: blood,Metabolome,Metabolomics,Metabolomics: methods,Multivariate Analysis,Phenotype,Polymorphism, Single Nucleotide,Regression Analysis},
month = {jul},
